- myAudit = Audit.new
%a{:href=>"/"}
  %h2.btn << AppGen

%h1.bigtitle NYC open dataset audit

%a.btn{:href=>"/audit/report"} About

- myAudit.json_obj.each do |superset|
  .supset
    %h1 
      =superset["title"]
    .hidden{ :style => 'display: none;'} 
      %p Shared schema:
      %code
        = superset["schema"]
      %ul
        - superset["datasets"].each do |set|
        %li
          %a{:href => 'https://data.cityofnewyork.us/-/-/' + set["id"]}
            = set["name"]

%script{ :src => "http://code.jquery.com/jquery-1.9.1.min.js" }
